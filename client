from socket import *

# Constant Var for N
DEFAULT_N = 15

# Establish socket connection
serverName = 'localhost'
serverPort = 7257
clientSocket = socket(AF_INET, SOCK_STREAM)
clientSocket.connect((serverName, serverPort))



